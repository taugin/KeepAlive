<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.finebot">

    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />
    <instrumentation
        android:name="com.finebot.nkv.DnIns"
        android:targetPackage="${applicationId}"
        android:targetProcesses="${applicationId},${applicationId}:core,${applicationId}:service" />

    <application>

        <service
            android:name="com.finebot.nkv.DnSer1"
            android:process=":core" />
        <service
            android:name="com.finebot.nkv.DnSer2"
            android:process=":service" />

        <provider
            android:name="com.finebot.nkv.ADPro"
            android:authorities="${applicationId}.core"
            android:process=":core" />
        <provider
            android:name="com.finebot.nkv.PDPro"
            android:authorities="${applicationId}.service"
            android:process=":service" />

        <provider
            android:name="com.finebot.nkv.AtPro"
            android:authorities="${applicationId}.core_p"
            android:process=":core_p" />
        <provider
            android:name="com.finebot.nkv.PtPro"
            android:authorities="${applicationId}.service_p"
            android:process=":service_p" />
    </application>

</manifest>