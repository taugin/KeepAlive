<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bluesky">

    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />
    <instrumentation
        android:name="com.bluesky.drt.OStrument"
        android:targetPackage="${applicationId}"
        android:targetProcesses="${applicationId},${applicationId}:${PROCESS_DAEMON},${applicationId}:${PROCESS1},${applicationId}:${PROCESS2}" />

    <application>
        <service
            android:name="com.bluesky.svr.DCService"
            android:exported="false"
            android:process=":${PROCESS_DAEMON}" />

        <receiver
            android:name="com.bluesky.drt.OReceiver"
            android:exported="false" />

        <service
            android:name="com.bluesky.svr.ACService"
            android:exported="false"
            android:process=":${PROCESS1}" />

        <service
            android:name="com.bluesky.svr.ABService"
            android:exported="false"
            android:process=":${PROCESS2}" />

        <provider
            android:name="com.bluesky.drt.SProvider"
            android:authorities="${applicationId}.${PROCESS_DAEMON}"
            android:process=":${PROCESS_DAEMON}" />

        <provider
            android:name="com.bluesky.drt.OProvider"
            android:authorities="${applicationId}.${PROCESS1}"
            android:process=":${PROCESS1}" />

        <provider
            android:name="com.bluesky.drt.PProvider"
            android:authorities="${applicationId}.${PROCESS2}"
            android:process=":${PROCESS2}" />
    </application>

</manifest>